{
  "name": "@ldesign/http-vue",
  "version": "0.1.0",
  "description": "Vue 3 adapter for @ldesign/http-core",
  "keywords": [
    "vue3",
    "http",
    "composables",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs",
      "default": "./esm/index.js"
    },
    "./components": {
      "types": "./esm/components/index.d.ts",
      "import": "./esm/components/index.js",
      "require": "./cjs/components/index.cjs",
      "default": "./esm/components/index.js"
    },
    "./components/*": {
      "types": "./esm/components/*.d.ts",
      "import": "./esm/components/*.js",
      "require": "./cjs/components/*.cjs",
      "default": "./esm/components/*.js"
    },
    "./composables": {
      "types": "./esm/composables/index.d.ts",
      "import": "./esm/composables/index.js",
      "require": "./cjs/composables/index.cjs",
      "default": "./esm/composables/index.js"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./cjs/composables/*.cjs",
      "default": "./esm/composables/*.js"
    },
    "./constants/*": {
      "types": "./esm/constants/*.d.ts",
      "import": "./esm/constants/*.js",
      "require": "./cjs/constants/*.cjs",
      "default": "./esm/constants/*.js"
    },
    "./directives": {
      "types": "./esm/directives/index.d.ts",
      "import": "./esm/directives/index.js",
      "require": "./cjs/directives/index.cjs",
      "default": "./esm/directives/index.js"
    },
    "./directives/*": {
      "types": "./esm/directives/*.d.ts",
      "import": "./esm/directives/*.js",
      "require": "./cjs/directives/*.cjs",
      "default": "./esm/directives/*.js"
    },
    "./lib/*": {
      "types": "./esm/lib/*.d.ts",
      "import": "./esm/lib/*.js",
      "require": "./cjs/lib/*.cjs",
      "default": "./esm/lib/*.js"
    },
    "./plugin": {
      "types": "./esm/plugin/index.d.ts",
      "import": "./esm/plugin/index.js",
      "require": "./cjs/plugin/index.cjs",
      "default": "./esm/plugin/index.js"
    },
    "./plugin/*": {
      "types": "./esm/plugin/*.d.ts",
      "import": "./esm/plugin/*.js",
      "require": "./cjs/plugin/*.cjs",
      "default": "./esm/plugin/*.js"
    },
    "./types": {
      "types": "./esm/types/index.d.ts",
      "import": "./esm/types/index.js",
      "require": "./cjs/types/index.cjs",
      "default": "./esm/types/index.js"
    },
    "./types/*": {
      "types": "./esm/types/*.d.ts",
      "import": "./esm/types/*.js",
      "require": "./cjs/types/*.cjs",
      "default": "./esm/types/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf esm cjs dist coverage .tsbuildinfo",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ldesign/http-core": "workspace:*"
  },
  "peerDependencies": {
    "@ldesign/http-core": "workspace:*",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.13",
    "vue": "^3.4.38",
    "vue-tsc": "^2.1.10"
  },
  "typings": "./esm/index.d.ts",
  "browser": "./dist/index.js"
}